{
  "name": "freedom-proxy",
  "packageManager": "yarn@3.5.1",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "build:server": "tsx build.ts",
    "build:web": "vite build",
    "build:runtime": "tsx build-runtime.ts",
    "watch:server": "chokidar --initial --silent \"src/**/*.ts\" \"shared/**/*.ts\" -c \"tsx build.ts\"",
    "dev:server": "tsx watch src/index.ts",
    "dev:web": "vite build --watch",
    "dev:runtime": "chokidar --initial --silent \"runtime/**/*.ts\" \"shared/**/*.ts\" -c \"tsx build-runtime.ts\"",
    "dev": "concurrently --kill-others \"npm:dev:server\" \"npm:dev:web\" \"npm:dev:runtime\"",
    "clean": "rimraf dist/"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@types/blocked-at": "^1.0.1",
    "@types/events": "^3",
    "@types/fs-extra": "^11.0.1",
    "@types/is-url": "^1.2.30",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/uglify-js": "^3.17.1",
    "@types/valid-data-url": "^2.0.0",
    "@types/ws": "^8.5.4",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "autoprefixer": "^10.4.14",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.0.1",
    "esbuild": "^0.17.19",
    "postcss": "^8.4.24",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.2",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "uglify-js": "^3.17.4",
    "user-agent-data-types": "^0.3.1",
    "vite": "^4.3.9",
    "vite-plugin-minify": "^1.5.2",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/websocket": "^8.1.0",
    "@tomphttp/bare-client": "^1.1.2-beta.3",
    "@tomphttp/bare-server-node": "^1.2.6",
    "arraybuffer-to-buffer": "^0.0.7",
    "await-to-js": "^3.0.0",
    "blocked-at": "^1.2.0",
    "browser-or-node": "^2.1.1",
    "buffer": "^6.0.3",
    "cash-dom": "^8.1.5",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.1.1",
    "events": "^3.3.0",
    "fastify": "^4.17.0",
    "fastify-graceful-shutdown": "^3.4.0",
    "fetch-cookie": "^2.1.0",
    "flowbite": "^1.6.5",
    "flowbite-react": "^0.4.6",
    "fs-extra": "^11.1.1",
    "import-meta-resolve": "^3.0.0",
    "is-relative-url": "^4.0.0",
    "is-url": "^1.2.4",
    "lodash-es": "^4.17.21",
    "modern-async": "^1.1.3",
    "msgpackr": "^1.9.2",
    "node-fetch": "^3.3.1",
    "normalize-url": "^8.0.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-if": "^4.1.4",
    "react-toastify": "^9.1.3",
    "store2": "^2.14.2",
    "tidy-url": "^1.6.15",
    "universal-user-agent": "^7.0.1",
    "util": "^0.12.5",
    "valid-data-url": "^4.0.1"
  },
  "alias": {
    "@sindresorhus/is": false
  }
}
